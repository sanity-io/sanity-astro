{
  "name": "@sanity/astro",
  "version": "1.3.0",
  "description": "Official Sanity Astro integration",
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf dist && vite build",
    "release": "semantic-release",
    "prepublishOnly": "npm run build"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/sanity-astro.js",
  "module": "./dist/sanity-astro.mjs",
  "files": [
    "dist",
    "studio"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/sanity-astro.mjs",
      "default": "./dist/sanity-astro.js"
    },
    "./studio/studio-route.astro": "./studio/studio-route.astro"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ],
    "exports": {
      "extra": {
        "./studio/studio-route.astro": "./studio/studio-route.astro"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-astro.git",
    "directory": "packages/sanity-astro"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/plugins/sanity-astro",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-astro/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@sanity/semantic-release-preset": "^4.1.4",
    "astro": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.1.1",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3"
  },
  "peerDependencies": {
    "astro": "^2.0.0 || ^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "*"
  },
  "keywords": [
    "astro-integration",
    "withastro",
    "sanity"
  ],
  "dependencies": {
    "@sanity/client": "^6.4.9",
    "@sanity/ui": "^1.8.2",
    "sanity": "*"
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
